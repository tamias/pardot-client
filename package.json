{
  "name": "pardot-client",
  "version": "0.0.1",
  "description": "A Pardot client for node",
  "main": "lib/index",
  "typings": "lib/index",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "check": "tsc --noEmit -p tsconfig.json",
    "lint": "eslint .",
    "test": "jest --coverage",
    "jest": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test && npm run check && npm run build"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tamias/pardot-client.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tamias/pardot-client/issues"
  },
  "homepage": "https://github.com/tamias/pardot-client#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "axios-auth-refresh": "^3.1.0",
    "qs": "^6.10.1",
    "simple-oauth2": "^4.2.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.1",
    "@types/simple-oauth2": "^4.1.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "axios-mock-adapter": "^1.19.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jest-formatting": "^3.0.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "husky": "^4.3.8",
    "jest": "^27.0.4",
    "prettier": "^2.2.1",
    "ts-expect": "^1.3.0",
    "ts-jest": "^27.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.3.2"
  }
}
